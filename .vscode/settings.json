{
  "cSpell.words": [
    "buble",
    "conventionalcommits",
    "cssnano",
    "favicons",
    "lintstagedrc",
    "packagejson",
    "pmmmwh",
    "postz",
    "postz:cz",
    "precommit",
    "prestart",
    "prez",
    "prez:cz",
    "svgr",
    "tanem",
    "toastify"
  ],
  "eslint.packageManager": "pnpm",
  "git.ignoreLimitWarning": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/dist": true,
    "**/coverage": true,
    "**/docs": true,
    "**/es": true,
    "**/pnpm-lock.yaml": true
  }
}
